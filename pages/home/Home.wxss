  background-color: transparent;
}

.header {
  height: 95rpx;
  background-image: linear-gradient(to top, #fdebe6 0%, #fdf0ea 100%);
  display: flex;
  position: fixed;
  align-items: center;
  justify-content: space-evenly;
  width: 100%;
  top: 0;
  z-index: 999;
}

.header .days {
  margin-left: 6rpx;
  display: flex;
  align-items: center;
}

.header .days .day {
  font-size: 55rpx;
  margin-right: 10rpx;
}

.header .days .year {
  font-size: 20rpx;
  line-height: 30rpx;
  color: #67635b;
}

.header .search-container {
  display: flex;
  justify-content: space-between;
  align-items: center;
  box-sizing: border-box;
  flex: 1;
  margin: auto;
  margin-left: 10rpx;
}

.header .search-container .search-left {
  background-color: white;
  flex: 2;
  border-radius: 40rpx;
  padding-right: 40rpx;
  box-sizing: border-box;
}

.header .search-container .search-left image {
  width: 48rpx;
  height: 48rpx;
  padding: 10rpx 10rpx;
  vertical-align: middle;
}

.header .search-container .search-left input {
  display: inline-block;
  vertical-align: middle;
  width: calc(100% - 88rpx);
}

.header .avatar {
  margin: auto 15rpx;
}

.header .avatar image {
  width: 60rpx;
  height: 60rpx;
  vertical-align: middle;
}

.goTop {
  position: fixed;
  bottom: 80rpx;
  right: 60rpx;
  z-index: 9;
}

.qiandao {
  position: fixed;
  bottom: 180rpx;
