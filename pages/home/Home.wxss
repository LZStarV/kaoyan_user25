  z-index: 9;
}

.endReached {
  height: 100rpx;
  line-height: 100rpx;
  color: #bdbdbd;
  font-size: 28rpx;
  text-align: center;
}

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100vh;
  background: linear-gradient(135deg, #8e44ad, #ff5252);
  padding: 20px;
}

.title {
  font-size: 24px;
  font-weight: bold;
  color: #ffffff;
  margin-bottom: 20px;
}

.sentence {
  font-size: 16px;
  color: #ffffff;
  text-align: center;
  margin-bottom: 15px;
  animation: fadeIn 1s ease-in forwards;
  background: rgba(0, 0, 0, 0.3);
  padding: 10px;
  border-radius: 10px;
  width: 80%;
}

.share-btn {
  background-color: #2ecc71;
  color: white;
  font-size: 16px;
  padding: 10px 20px;
  border-radius: 5px;
  margin-top: 20px;
}

